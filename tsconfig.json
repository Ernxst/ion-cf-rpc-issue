{
	"compilerOptions": {
		"lib": ["esnext", "dom", "dom.iterable"],
		"module": "esnext",
		"target": "esnext",
		"verbatimModuleSyntax": true,
		"moduleResolution": "bundler",
		"skipLibCheck": true,
		"strict": true,
		"baseUrl": ".",
		"outDir": "./dist",
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["./functions/**/*.ts", "sst.config.ts"],
	"exclude": ["node_modules/**/*", ".sst/**/*"]
}
